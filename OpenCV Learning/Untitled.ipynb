    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "img = np.array([[0.1, 0.1, 0.1],\n",
    "              [0.9, 0.9, 0.9]])\n",
    "print(img.shape)\n",
    "\n",
    "img = cv2.resize(img, (300,200))\n",
    "\n",
    "cv2.imshow('hi', img)\n",
    "\n",
    "cv2.waitKey(0) # waits until a key is pressed\n",
    "cv2.destroyAllWindows() # destroys the window showing image\n"
   ]
